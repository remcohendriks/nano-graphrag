{
    "mcpServers": {
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp@latest"
            ]
        },
        "puppeteer": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-puppeteer@latest"
            ]
        },
        "neo4j-database": {
            "command": "uvx",
            "args": [
                "mcp-neo4j-cypher@0.4.0",
                "--transport",
                "stdio"
            ],
            "env": {
                "NEO4J_URI": "bolt://localhost:7687",
                "NEO4J_USERNAME": "neo4j",
                "NEO4J_PASSWORD": "your-secure-password-change-me",
                "NEO4J_DATABASE": "neo4j"
            }
        }
    }
}